using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp2
{
    class Program
    {
        static void Main(string[] args)
        {
            for (int i = 1; i <= 10; i++) // lines
            {
                for (int n = 1; n <= 10; n++)
                {
                    Console.Write("*");
                }
                Console.WriteLine();
            }
        }
    }
}
